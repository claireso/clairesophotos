{
  "extends": "./.babelrc",
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      },
    ]
  ],
  "plugins": [
    "dynamic-import-node"
  ],
  "ignore": [
    "src/static/**/*.js",
    "src/coverage/**/*.js",
    "src/**/__tests__/*.js"
  ]
}