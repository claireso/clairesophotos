{
  "presets": ["es2015", "react"],
  "plugins": [
    "transform-object-rest-spread",
    "transform-class-properties",
    ["babel-plugin-styled-components", {
      "ssr": true,
      "displayName": true
    }]
  ],
  "env": {
    "production": {
      "plugins": [
        ["babel-plugin-styled-components", {
          "ssr": true,
          "displayName": false
        }]
      ]
    }
  }
}
