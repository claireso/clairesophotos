{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    ["babel-plugin-styled-components", {
      "ssr": true,
      "displayName": true
    }]
  ],
  "env": {
    "production": {
      "plugins": [
        ["babel-plugin-styled-components", {
          "ssr": true,
          "displayName": false
        }]
      ]
    }
  }
}
