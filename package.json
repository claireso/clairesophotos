{
  "name": "journal",
  "version": "1.0.0",
  "description": "A self-hosted website to show your photographs",
  "private": true,
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "dev": "nodemon --config nodemon.json src/server/server.js",
    "server": "node ./dist/server/server.js",
    "watch": "webpack --config ./webpack/development.js --watch",
    "build:static": "webpack --config ./webpack/production.js --bail",
    "build:server": "babel --config-file ./.babel-server.babelrc ./src --out-dir ./dist",
    "build": "npm run build:server && npm run build:static",
    "lint": "eslint src/.",
    "lint:write": "eslint src/. --fix",
    "prettier": "prettier --write \"src/**/*.js\"",
    "bootstrap": "babel-node scripts/bootstrap.js --config-file ./.babel-server.babelrc",
    "test": "IS_PUSH_ENABLED=true jest --coverage",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/claireso/journal.git"
  },
  "author": "Claire Sosset",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/claireso/journal/issues"
  },
  "homepage": "https://github.com/claireso/journal#readme",
  "dependencies": {
    "@babel/polyfill": "7.8.3",
    "@loadable/component": "5.12.0",
    "@loadable/server": "5.12.0",
    "@reach/router": "1.2.1",
    "axios": "0.19.1",
    "body-parser": "1.19.0",
    "date-fns": "2.9.0",
    "express": "4.17.1",
    "express-session": "1.17.0",
    "helmet": "3.21.2",
    "intersection-observer": "0.7.0",
    "jimp": "0.9.3",
    "jsonfile": "5.0.0",
    "lodash": "4.17.15",
    "multer": "1.4.2",
    "passport": "0.4.1",
    "passport-local": "1.0.0",
    "pg": "7.17.1",
    "prop-types": "15.7.2",
    "qs": "6.9.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-redux": "7.1.3",
    "react-spring": "8.0.27",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "styled-components": "5.0.0",
    "ulid": "2.3.0",
    "web-push": "3.4.3"
  },
  "devDependencies": {
    "@babel/cli": "7.8.3",
    "@babel/core": "7.8.3",
    "@babel/node": "7.8.3",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/preset-env": "7.8.3",
    "@babel/preset-react": "7.8.3",
    "@loadable/babel-plugin": "5.12.0",
    "@loadable/webpack-plugin": "5.12.0",
    "@testing-library/jest-dom": "5.0.0",
    "@testing-library/react": "9.4.0",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "babel-plugin-dynamic-import-node": "2.3.0",
    "babel-plugin-module-resolver": "4.0.0",
    "babel-plugin-styled-components": "1.10.6",
    "chalk": "3.0.0",
    "compression-webpack-plugin": "3.1.0",
    "copy-webpack-plugin": "5.1.1",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "husky": "4.0.10",
    "jest": "24.9.0",
    "jest-fetch-mock": "3.0.1",
    "jest-styled-components": "7.0.0",
    "lint-staged": "10.0.0",
    "pgtools": "0.3.0",
    "prettier": "1.19.1",
    "promptly": "3.0.3",
    "react-styleguidist": "10.3.2",
    "redux-devtools-extension": "2.13.8",
    "redux-mock-store": "1.5.4",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10",
    "webpack-manifest-plugin": "2.2.0",
    "webpack-merge": "4.2.2",
    "workbox-webpack-plugin": "4.3.1"
  }
}