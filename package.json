{
  "name": "journal",
  "version": "1.0.0",
  "description": "A self-hosted website to show your photography",
  "private": true,
  "scripts": {
    "postinstall": "cp ./node_modules/sw-toolbox/sw-toolbox.js ./public",
    "dev": "nodemon -e js,html,pug src/server/server.js --exec babel-node",
    "server": "node ./dist/server/server.js",
    "watch": "webpack --config ./webpack/development.js --watch",
    "build:static": "webpack --config ./webpack/production.js --bail",
    "build:server": "babel ./src --out-dir ./dist",
    "build": "npm run build:server && npm run build:static",
    "eslint": "eslint src/.",
    "prettier": "prettier --write \"src/**/*.js\"",
    "bootstrap": "babel-node scripts/bootstrap.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/claireso/journal.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/claireso/journal/issues"
  },
  "homepage": "https://github.com/claireso/journal#readme",
  "dependencies": {
    "@babel/polyfill": "7.0.0",
    "@reach/router": "1.1.1",
    "axios": "0.18.0",
    "body-parser": "1.18.3",
    "date-fns": "1.29.0",
    "express": "4.16.3",
    "express-session": "1.15.6",
    "helmet": "3.13.0",
    "intersection-observer": "0.5.0",
    "jsonfile": "5.0.0",
    "lodash": "4.17.11",
    "multer": "1.3.1",
    "passport": "0.4.0",
    "passport-local": "1.0.0",
    "pg": "7.4.3",
    "prop-types": "15.6.2",
    "qs": "6.5.2",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-pose": "3.3.4",
    "react-redux": "5.0.7",
    "redux": "4.0.0",
    "redux-thunk": "2.3.0",
    "styled-components": "4.0.0-beta.1",
    "sw-toolbox": "3.6.0",
    "ulid": "2.3.0",
    "web-push": "3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "7.1.0",
    "@babel/core": "7.1.0",
    "@babel/node": "7.0.0",
    "@babel/plugin-proposal-class-properties": "7.1.0",
    "@babel/preset-env": "7.1.0",
    "@babel/preset-react": "7.0.0",
    "babel-eslint": "9.0.0",
    "babel-loader": "8.0.2",
    "babel-plugin-styled-components": "1.6.4",
    "chalk": "2.4.1",
    "compression-webpack-plugin": "2.0.0",
    "copy-webpack-plugin": "4.5.2",
    "eslint": "5.6.0",
    "eslint-config-prettier": "3.0.1",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-react": "7.11.1",
    "htpasswd": "2.4.0",
    "minimist": "1.2.0",
    "pgtools": "0.3.0",
    "prettier": "1.14.2",
    "promptly": "3.0.3",
    "redux-devtools-extension": "2.13.5",
    "webpack": "4.19.1",
    "webpack-cli": "3.1.0",
    "webpack-manifest-plugin": "2.0.4",
    "webpack-merge": "4.1.4"
  }
