{
  "name": "journal",
  "version": "1.0.0",
  "description": "A self-hosted website to show your photography",
  "private": true,
  "scripts": {
    "dev": "nodemon --config nodemon.json src/server/server.js",
    "server": "node ./dist/server/server.js",
    "watch": "webpack --config ./webpack/development.js --watch",
    "build:static": "webpack --config ./webpack/production.js --bail",
    "build:server": "babel ./src --out-dir ./dist --env-name serverProd --ignore \"src/**/__tests__/*.js\"",
    "build": "npm run build:server && npm run build:static",
    "eslint": "eslint src/.",
    "prettier": "prettier --write \"src/**/*.js\"",
    "bootstrap": "babel-node scripts/bootstrap.js",
    "test": "IS_PUSH_ENABLED=true jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/claireso/journal.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/claireso/journal/issues"
  },
  "homepage": "https://github.com/claireso/journal#readme",
  "dependencies": {
    "@babel/polyfill": "7.2.5",
    "@reach/router": "1.2.1",
    "axios": "0.18.0",
    "body-parser": "1.18.3",
    "date-fns": "1.30.1",
    "express": "4.16.4",
    "express-session": "1.15.6",
    "helmet": "3.15.0",
    "intersection-observer": "0.5.1",
    "jest-styled-components": "6.3.1",
    "jimp": "0.6.0",
    "jsonfile": "5.0.0",
    "lodash": "4.17.11",
    "multer": "1.4.1",
    "passport": "0.4.0",
    "passport-local": "1.0.0",
    "pg": "7.8.0",
    "prop-types": "15.6.2",
    "qs": "6.6.0",
    "react": "16.7.0-alpha.0",
    "react-dom": "16.7.0-alpha.0",
    "react-loadable": "5.5.0",
    "react-redux": "6.0.0",
    "react-spring": "7.2.10",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0",
