{
  "name": "journal",
  "version": "1.0.0",
  "description": "A self-hosted website to show your photography",
  "private": true,
  "scripts": {
    "postinstall": "cp ./node_modules/sw-toolbox/sw-toolbox.js ./public",
    "dev": "nodemon -e js,html,pug src/server/server.js --exec babel-node",
    "server": "node ./dist/server/server.js",
    "watch": "webpack --config ./webpack/config.babel.js --watch",
    "build:static": "webpack --config ./webpack/config.babel.js --bail",
    "build:server": "babel ./src --out-dir ./dist",
    "build": "npm run build:server && npm run build:static",
    "eslint": "eslint src/.",
    "prettier": "prettier --write \"src/**/*.js\"",
    "bootstrap": "babel-node scripts/bootstrap.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/claireso/journal.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/claireso/journal/issues"
  },
  "homepage": "https://github.com/claireso/journal#readme",
  "dependencies": {
    "@reach/router": "1.1.1",
    "axios": "0.18.0",
    "body-parser": "1.18.3",
    "date-fns": "1.29.0",
    "express": "4.16.3",
    "express-session": "1.15.6",
    "helmet": "3.12.1",
    "jsonfile": "4.0.0",
    "lodash": "4.17.10",
    "multer": "1.3.1",
    "passport": "0.4.0",
    "passport-local": "1.0.0",
    "pg": "7.4.3",
    "prop-types": "15.6.2",
    "qs": "6.5.2",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-redux": "5.0.7",
    "redux": "4.0.0",
    "redux-thunk": "2.3.0",
    "sw-toolbox": "3.6.0",
    "ulid": "2.3.0",
    "web-push": "3.3.2"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.6",
    "babel-loader": "7.1.5",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "chalk": "2.4.1",
    "eslint": "5.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-react": "7.10.0",
    "htpasswd": "2.4.0",
    "minimist": "1.2.0",
    "pgtools": "0.3.0",
    "prettier": "1.13.7",
    "promptly": "3.0.3",
    "redux-devtools-extension": "2.13.5",
    "webpack": "4.16.1",
    "webpack-cli": "3.0.8",
    "webpack-manifest-plugin": "2.0.3"
  }
}
